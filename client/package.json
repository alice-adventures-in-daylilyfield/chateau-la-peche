{
  "name": "client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -fr dist/",
    "jade": "jade -o dist/ src/index.jade",
    "jadew": "jade -w -o dist/ src/index.jade",
    "sass": "node-sass src/index.sass dist/index.css",
    "sassw": "node-sass -w src/index.sass dist/index.css",
    "js": "browserify src/index.js -d -o dist/bundle.js -t [ babelify --presets [ es2015 ] ]",
    "jsw": "watchify src/index.js -v -d -o dist/bundle.js -t [ babelify --presets [ es2015 ] ]",
    "test": "mocha --require intelli-espower-loader spec/**/*.spec.js",
    "build": "concurrently 'npm run jade' 'npm run sass' 'npm run js'",
    "dev": "concurrently 'npm run jadew' 'npm run sassw' 'npm run jsw'"
  },
  "directories": {
    "test": "spec/"
  },
  "author": "daylilyfield",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "concurrently": "^2.0.0",
    "intelli-espower-loader": "^1.0.1",
    "jade": "^1.11.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "power-assert": "^1.3.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "mithril": "^0.2.3"
  }
}
